{
    "name": "eatico",
    "version": "0.0.1",
    "scripts": {
        "build": "npm run build-client",
        "build-client": "npm run wp -- .webpack/dev.config.js",
        "dev": "node ./node_modules/.bin/concurrent --kill-others \"npm run watch-client\"",
        "prod": "npm run wp -- .webpack/prod.config.js",
        "test": "NODE_ENV=\"test\" node ./node_modules/mocha/bin/mocha --ui bdd --compilers js:src/__tests__/bootstrap.js 'src/**/*.spec.js'",
        "watch-client": "NODE_ENV=\"development\" NODE_PATH=\"./src\" node .webpack/webpack.server.js",
        "wp": "node ./node_modules/.bin/webpack --verbose --colors --display-error-details --config"
    },
    "dependencies": {
        "classnames": "2.2.1",
        "firebase": "2.4.0",
        "history": "^1.13.1",
        "immutable": "^3.7.5",
        "less": "*",
        "moment": "^2.10.6",
        "react": "^0.14.7",
        "react-dom": "^0.14.2",
        "react-google-maps": "^4.7.2",
        "react-redux": "^3.0.0",
        "react-select": "1.0.0-beta1",
        "redux": "^3.0.0",
        "routex": "^1.0.0-alpha.1",
        "superagent": "*",
        "superagent-bluebird-promise": "^2.0.2",
        "velocity-react": "^1.1.3"
    },
    "devDependencies": {
        "babel-cli": "6.1.18",
        "babel-core": "6.2.1",
        "babel-eslint": "4.1.6",
        "babel-loader": "6.2.0",
        "babel-preset-es2015": "6.1.18",
        "babel-preset-react": "6.1.18",
        "babel-preset-stage-0": "6.1.18",
        "babel-preset-es2015-node4": "2.0.1",
        "babel-plugin-css-modules-transform": "0.0.4",
        "compression-webpack-plugin": "0.2.0",
        "concurrently": "1.0.0",
        "css-loader": "0.23.0",
        "extract-text-webpack-plugin": "0.9.1",
        "file-loader": "0.8.5",
        "image-webpack-loader": "*",
        "json-loader": "0.5.4",
        "less-loader": "*",
        "react-addons-test-utils": "0.14.3",
        "react-hot-loader": "2.0.0-alpha-4",
        "strip-loader": "0.1.0",
        "style-loader": "0.13.0",
        "url-loader": "0.5.7",
        "webpack": "1.12.9",
        "webpack-dev-server": "1.14.0"
    }
}
